#!/bin/sh

if [ -f /etc/ufw/after.rules-ufw-docker* ]; then
	echo "Found after.rules backup"
	echo "Resetting after.rules to backup"
	rm /etc/ufw/after.rules
	mv /etc/ufw/after.rules-ufw-docker* /etc/ufw/after.rules
else
	echo "Didn't find an after.rules backup. You may want to manually edit your"
	echo "/etc/ufw/after.rules if you ever ran ufw-docker install."
fi

exit 0

